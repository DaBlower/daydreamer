resource_name = "scene.gd"
script/source = "extends Node2D

var shop = preload(\"res://scenes/shop.tscn\").instantiate()
var shown: bool = false

func _ready() -> void:
	var shop_layer = CanvasLayer.new()
	add_child(shop_layer)
	shop_layer.add_child(shop)
	
	shop.visible = false

func _process(delta: float) -> void:
	if Input.is_action_just_pressed(\"show_shop\"):
		shown = !shown
		shop.visible = shown
		Global.toggle_game_status(shown)
"

